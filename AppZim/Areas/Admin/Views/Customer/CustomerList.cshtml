@using AppZim.TableSql
@using AppZim.Models
@using MB.Extensions
@using System.Text;
@using System.Collections.Generic;
@model List<getallcustomernew_Result>
@{
    ViewBag.Title = "CustomerList";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdminNew.cshtml";
    var aclog = AccountTable.getbyID(Session["UID"].ToString().ToInt(0));
    List<tbl_Account> lsale = AccountTable.getall().Where(n => n.Status == 2 && (n.RoleID == 3 || n.RoleID == 6)).OrderBy(n => n.FullName).ToList();
    List<tbl_AcademicPurposes> mucdich = AcademicPurposesTable.getall();
    string[] strText = new string[4] { "Trang đầu", "Trang cuối", "Trang sau", "Trang trước" };
    var lLanguage = Language.GetAll();
    List<tbl_SourceOfCustomer> sc = SourceOfCustomerTable.getall();
    List<tbl_Job> jobs = JobTable.getall();
    List<tbl_City> citys = LocationTable.getall();
    StringBuilder tvv = new StringBuilder();
    StringBuilder nguon = new StringBuilder();
    foreach (var item in sc)
    {
        nguon.Append("<option value=\"" + item.ID + "\">" + item.SourceOfCustomer + "</option>");
    }
    StringBuilder viec = new StringBuilder();
    foreach (var item in jobs)
    {
        viec.Append("<option value=\"" + item.ID + "\">" + item.JobName + "</option>");
    }
    StringBuilder tinh = new StringBuilder();
    foreach (var item in citys)
    {
        tinh.Append("<option value=\"" + item.ID + "\">" + item.CityName + "</option>");
    }
    foreach (var item in lsale)
    {
        tvv.Append("<option value=\"" + item.ID + "\">" + item.FullName + "</option>");
    }
    StringBuilder mdich = new StringBuilder();
    foreach (var item in mucdich)
    {
        mdich.Append("<option value=\"" + item.ID + "\">" + item.AcademicPurposesName + "</option>");
    }
    var trinhdo = AcademicLevelTable.getall();
    var nations = NationTable.GetAll();
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb breadcrumb-style1 mg-b-30">
        <li class="breadcrumb-item active" aria-current="page">Danh sách học viên</li>
    </ol>
</nav>
@using (Html.BeginForm("CustomerList", "Customer", FormMethod.Get, new { id = "form-search" }))
{
    <div class="d-xl-flex align-items-center justify-content-between ">
        <div class="d-xl-block">
            <div class="d-flex justify-content-xl-center mg-t-20 mg-xl-t-0 flex-wrap">
                <div class="search-form mg-l-5 mg-r-5">
                    <input type="text" name="search" class="form-control" placeholder="Tìm kiếm..." value="@ViewBag.Search">
                    <button class="btn" type="button" id="btn-search"><i data-feather="search"></i></button>
                </div>
                
                <a href="#filterCollapse" class="btn btn-primary mg-l-5 mg-r-5 mg-sm-r-0" role="button" data-toggle="collapse" aria-expanded="false" aria-controls="filterCollapse">
                    <i data-feather="filter"></i>
                </a>
            </div>
        </div>
    </div>
    <div class="collapse mg-t-10" id="filterCollapse">
        <div class="card">
            <div class="card-body">
                <div class="row flex-wrap">
                    <div class="form-group col-md-4 col-6 col-xl-3">
                        <label class="form-control-label">Tình trạng học tập</label>
                        <select class="form-control select2" name="slStudy">
                            <option value="0">-- Tất cả --</option>
                            <option value="1">Chưa học</option>
                            <option value="2">Đang học</option>
                            <option value="3">Đã học xong</option>
                        </select>
                        <script>$('select[name=slStudy]').val(@ViewBag.Study)</script>
                    </div>

                    <div class="form-group col-md-4 col-6 col-xl-3">
                        <label class="form-control-label">Trạng thái:</label>
                        <select class="form-control select2" name="status">
                            <option value="0">-- Tất cả --</option>
                            @{
                                if (ViewBag.StatusID != null)
                                {
                                    if (ViewBag.StatusID == 1)
                                    {
                                        <option value="1" selected>Chưa kích hoạt</option>
                                    }
                                    else
                                    {
                                        <option value="1">Chưa kích hoạt</option>
                                    }
                                    if (ViewBag.StatusID == 2)
                                    {
                                        <option value="2" selected>Hoạt động</option>
                                    }
                                    else
                                    {
                                        <option value="2">Hoạt động</option>
                                    }
                                    if (ViewBag.StatusID == 3)
                                    {
                                        <option value="3" selected>Khóa</option>
                                    }
                                    else
                                    {
                                        <option value="3">Khóa</option>
                                    }
                                }
                                else
                                {
                                    <option value="1">Chưa kích hoạt</option>
                                    <option value="2">Hoạt động</option>
                                    <option value="3">Khóa</option>
                                }
                            }
                        </select>
                    </div>

                    <div class="form-group col-md-4 col-6 col-xl-3">
                        <label>Thao tác:</label>
                        <div>
                            <button type="submit" class="btn btn-primary"><i data-feather="search"></i> Tìm kiếm</button>
                            <a href="~/app-assets/form.xlsx" download="download" class="btn btn-primary" style="background-color: cornflowerblue; border-color: cornflowerblue;" data-toggle="tooltip" data-placement="left" title="Mẫu excel"><i class="far fa-save"></i></a>
                            @using (Html.BeginForm("ImportExcel", "Customer", FormMethod.Post, new { id = "f-import", @enctype = "multipart/form-data" }))
                            {
                                <label class="thumbnail-upload">
                                    <input type="file" name="file" class="hidden upload-image-input" onchange="Submit()">
                                    <span class="pos-relative rounded-5 overflow-hidden btn btn-primary waves-light"
                                          data-toggle="tooltip" data-placement="left" title="Upload file excel">
                                        <i class="far fa-file-excel"></i>
                                    </span>
                                </label>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
