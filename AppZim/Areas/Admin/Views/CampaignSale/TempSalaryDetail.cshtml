@using AppZim.Models
@model List<Admin_RevenueDetail_GetByTarget_Result>
@{
    ViewBag.Title = "TempSalaryDetail";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdminNew.cshtml";
    string[] strText = new string[4] { "Trang đầu", "Trang cuối", "Trang sau", "Trang trước" };
}
<!-- Breadcrumb -->
<nav aria-label="breadcrumb">
    <ol class="breadcrumb breadcrumb-style1 mg-b-30">
        <li class="breadcrumb-item active" aria-current="page">Chi tiết doanh thu</li>
    </ol>
</nav>
<!-- End breadcrumb -->
<div class="customer-table mg-t-30">
    <div class="table-responsive">
        <table class="table table-striped table-vcenter">
            <thead class="thead-light">
                <tr>
                    <th>Học viên</th>
                    <th>Số điện thoại</th>
                    <th>Gói cam kết</th>
                    <th>Mã khuyến mãi</th>
                    <th>Tổng tiền</th>
                    <th>Nợ</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var i in Model)
                {
                    <tr>
                        <td class="no-wrap">@i.StudentName</td>
                        <td class="no-wrap">@i.StudentPhone</td>
                        <td class="no-wrap">@i.CommitmentName - @String.Format("{0:0,0}", i.CommitmentPrice)</td>
                        <td>@i.DiscountCode - @String.Format("{0:0,0}", i.DiscountPrice)</td>
                        <td>@String.Format("{0:0,0}", i.CustomPrice)</td>
                        <td>@String.Format("{0:0,0}", i.PriceLeft)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<nav aria-label="Page navigation">
    <ul class="pagination mg-b-0 justify-content-end">
        <div class="pagination-container">
            @PhanTrang.PhanTrangCustom(10, ViewBag.Page, ViewBag.PageCount, Request.RawUrl, strText)
        </div>
    </ul>
</nav>
