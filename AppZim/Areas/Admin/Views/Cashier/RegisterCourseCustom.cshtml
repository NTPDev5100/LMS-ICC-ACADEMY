        function LoadPrice() {

            let priceMin = 0;
            let priceMax = 0;
            $("#ddl-class-course :selected").each(function () {
                priceMin += $(this).data('min');
                priceMax += $(this).data('max');
            });

            let moneyUnit = $('select[name=MoneyExchangeRate]').val();
            let money = $('#totalcourse').val().replace(/,/g, '');
            let commitmentPrice = $('select[name=commitmentpackage] :selected').data('price');
            let discountPrice = parseInt($('input[name=txt-discount-price]').val());
            let payment = (money * moneyUnit) + commitmentPrice - discountPrice;

            if ((priceMin + commitmentPrice - discountPrice) > payment || (priceMax + commitmentPrice - discountPrice) < payment) {
                toast.create({
                    title: 'Thông báo!',
                    text: 'Tổng tiền phải nằm trong khoảng Giá Min + gói cam kết - Giá KM => Giá Max + gói cam kết - Giá KM',
                    icon: 'notifications_active',
                    classBackground: 'noti-warning',
                    timeout: 4000
                });
                $('select[name=MoneyExchangeRate]').val(0);
                $('select[name=MoneyExchangeRate]').trigger('change');
                $('#totalcourse').val(0);
                $('#totalcourse').focus();
                return;
            }

            $('#totalcourse').val(dinhdangtien(payment, ','));
        }
    </script>
